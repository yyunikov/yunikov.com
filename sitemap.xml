---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" 
  xmlns:xhtml="http://www.w3.org/1999/xhtml"
  xmlns:mobile="http://www.google.com/schemas/sitemap-mobile/1.0">
  <url>
    <loc>{{ site.url }}</loc>
    <priority>1.0</priority>
    <mobile:mobile/>
  </url>

  {% for dynamicPage in site.dynamicPages %}
    <url>
      <loc>{{ site.url }}/#!/{{ dynamicPage }}</loc>
      <priority>0.9</priority>
      <mobile:mobile/>
    </url>
  {% endfor %}

  {% for presentation in site.data.presentations %}
    <url>
      <loc>{{ site.url }}/#!/{{ presentation.url }}</loc>
      <priority>0.8</priority>
      <mobile:mobile/>
    </url>
  {% endfor %}
</urlset>